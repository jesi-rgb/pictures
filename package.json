{
  "name": "paytest",
  "version": "1.0.0",
  "description": "A blank template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation NODE_ENV=production next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=\"--no-deprecation --no-experimental-strip-types\" pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "reset-password": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env src/scripts/reset-password.ts",
    "sync-photos": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env src/scripts/sync-photos.ts",
    "fix-duplicate-filenames": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env src/scripts/fix-duplicate-filenames.ts",
    "db:dump": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env src/scripts/dump-database.ts",
    "db:restore": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env.production src/scripts/restore-database.ts",
    "sync:r2": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env.production src/scripts/upload-to-r2.ts",
    "migrate:prod": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env.production src/scripts/migrate-to-production.ts",
    "validate:prod": "cross-env NODE_OPTIONS=--no-deprecation tsx --env-file=.env.production src/scripts/validate-deployment.ts"
  },
  "dependencies": {
    "@payloadcms/db-postgres": "3.65.0",
    "@payloadcms/next": "3.65.0",
    "@payloadcms/richtext-lexical": "3.65.0",
    "@payloadcms/storage-s3": "3.65.0",
    "@payloadcms/ui": "3.65.0",
    "@tailwindcss/postcss": "^4.1.17",
    "cross-env": "^7.0.3",
    "daisyui": "^5.5.5",
    "dotenv": "16.4.7",
    "exifr": "^7.1.3",
    "graphql": "^16.8.1",
    "next": "^16.1.0",
    "payload": "3.65.0",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "0.34.2",
    "tailwindcss": "^4.1.17"
  },
  "devDependencies": {
    "@playwright/test": "1.56.1",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "@vitejs/plugin-react": "4.5.2",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "prettier": "^3.4.2",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.24.0+sha512.01ff8ae71b4419903b65c60fb2dc9d34cf8bb6e06d03bde112ef38f7a34d6904c424ba66bea5cdcf12890230bf39f9580473140ed9c946fef328b6e5238a345a"
}
